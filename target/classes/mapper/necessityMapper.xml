<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.NecessityMapper">

    <select id="getCourseByDept" resultType="java.lang.Long" parameterType="int">
        select course_id
        from necessity
        where dept_id = #{deptId};
    </select>

    <delete id="deleteNecessity" parameterType="java.lang.Long">
        delete from necessity
        where course_id = #{courseId};
    </delete>

</mapper>